{
  "version": 2,
  "builds": [
    { "src": "api/**/*.py", "use": "@vercel/python" },
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/api/preprocess", "dest": "/api/preprocess" },
    { "src": "/api/analyze", "dest": "/api/analyze" },
    { "src": "/(.*)", "dest": "/" }
  ],
  "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
}
